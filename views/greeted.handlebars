<h3  class='text-center text-primary'>Greeted users</h3>
<div class="row">
      <div class="col align-self-start"><a href="/" class='btn btn btn-primary'>Take me home</a></div>
       
    </div>
<div class="row">
    
   
    <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Language</th>
      <th scope="col">Greeted times</th>
      <th scope="col">counter links</th>
      <th scope="col">delete user</th>
    </tr>
  </thead>
  <tbody>
      {{#each database}}
    <tr>
      <td>{{users_greeted}}</td>
      <td>{{user_language}}</td>
      <td>{{counter}}</td>
      <td><a href="/counter/{{users_greeted}}" class="btn btn-light ">link</a></td>
      <td><a href="/remove/{{users_greeted}}" class="text-danger "><h4>X</h4></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
<div class="col align-self-end"><a href="/reset" class='btn bg-warning mb-5 text-dark'>clear database</a></div>